        <description>Client-side logic for AngularJS test app</description>
        <global>false</global>
        <name>x_306037_angularjs.angularjs_app_client_</name>
        <script><![CDATA[var app = angular.module('angularjsApp', []);
app.controller('AngularAppCtrl', function($scope) {
	var $ctrl = this;
	
	$ctrl.myVar = '';
	$ctrl.i = 0;
	
	$ctrl.clearText = clearText;
	
	function clearText() {
		$ctrl.myVar = '';
		$ctrl.i++;
		alert("The myVar variable was cleared " + $ctrl.i + " times by " + g_user.userName + ".");
	}
});]]></script>
